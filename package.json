    "eslint": "^8.22.0",
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.0",
    "pnpm": "^7.9.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.10.1",
    "sass": "^1.56.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.7",
    "vitest": "^0.22.0"
